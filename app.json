{
  "expo": {
    "name": "SAYLANIQUIZ",
    "slug": "SAYLANIQUIZ",
    "version": "1.1.0",  
    "orientation": "portrait",
    "icon": "./assets/images/quizicon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2"  
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/saylanicon.png",
        "backgroundColor": "#fff"
      },
      "package": "com.zeeshan.junaid.SAYLANIQUIZ",
      "versionCode": 2  
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/quizicon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/saylanicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "abfac7dc-6844-4312-97c5-16028e391451"
      }
    }
  }
}
